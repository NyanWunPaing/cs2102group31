<table class="table">
	<thead>
		<tr>
			<th scope="col">Uid</th>
			<th scope="col">Name</th>
			<th scope="col">Username</th>
		</tr>
	</thead>
	<tbody>
		<% for(var i=0; i< usersInfo.length; i++) { %>
		<tr>
			<td><%= usersInfo[i].uid%></td>
			<td><%= usersInfo[i].name%></td>
			<td><%= usersInfo[i].username%></td>
		</tr>
		<% } %>
	</tbody>
</table>

// GET STATEMENTS
function selectUsers(req, res, next) {
	caller.query(sql.query.all_users, (err, data) => {
        if(err){
            return next(error);
        }
        req.usersInfo = data.rows;
        return next();
	});
}